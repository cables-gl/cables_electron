{
  "name": "cables_standalone",
  "version": "0.0.1",
  "author": "undefined development",
  "description": "current templates for the different exe-exports in cables.gl",
  "repository": {
    "type": "git",
    "url": "git@github.com:cables-gl/cables_electron.git"
  },
  "license": "MIT",
  "main": "src/electron/main.js",
  "type": "module",
  "engines": {
    "node": "18.14.2"
  },
  "scripts": {
    "start": "electron .",
    "build": "gulp build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "pack": "electron-builder --dir",
    "p": "electron-builder --dir --config -c.mac.identity=null",
    "dist": "electron-builder"
  },
  "build": {
    "productName": "cables.gl standalone",
    "appId": "gl.cables.standalone",
    "files": [
      "**/*",
      "!gen/",
      "!.nvmrc",
      "!gulpfile.js",
      "!webpack.*.config.js"
    ],
    "extraResources": [
      {
        "from": "../cables/src/ops/base/",
        "to": "cables/src/ops/base/"
      },
      {
        "from": "../cables_api/public/libs/",
        "to": "public/libs/"
      },
      {
        "from": "../cables_api/public/libs/",
        "to": "public/libs_core/"
      },
      {
        "from": "../shared/client",
        "to": "shared/client"
      }
    ],
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "nodeVersion": "18.14.2",
    "electronLanguages": "en"
  },
  "dependencies": {
    "pako": "2.1.0",
    "marked": "1.2.9",
    "jsonfile": "6.1.0",
    "cables-shared": "file:../shared",
    "electron": "29.0.0"
  },
  "devDependencies": {
    "electron-builder": "24.12.0",
    "git-repo-info": "2.1.1",
    "gulp": "4.0.2",
    "webpack": "5.75.0",
    "webpack-stream": "7.0.0"
  }
}
